<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.benjen.flowchart">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="org.apmem.tools.example.MyActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="org.apmem.tools.example.FlowLayoutActivity"
                  android:label="@string/flow_layout_activity"/>
        <activity android:name="org.apmem.tools.example.FlowLayoutManagerActivity"
                  android:label="@string/flow_layout_manager_activity"/>
    </application>

</manifest>