<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:paddingLeft="@dimen/w_12"
    android:paddingRight="@dimen/w_12"
    android:paddingTop="@dimen/h_10">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/item_photo_clazz_IvImg"
            android:layout_width="@dimen/w_60"
            android:layout_height="@dimen/h_60"
            android:src="@drawable/empty_ic_no_data"
            />

        <TextView
            android:id="@+id/item_photo_clazz_TvTitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/w_10"
            android:layout_gravity="center_vertical"
            android:text="全部图片"
            />
    </LinearLayout>
    <View style="@style/line_normal_l"
        android:layout_marginTop="@dimen/h_10"/>
</LinearLayout>